package com.ruyicai.mgr.controller.dto;

import java.math.BigDecimal;
import java.util.Date;

import org.springframework.roo.addon.javabean.RooJavaBean;
import org.springframework.roo.addon.json.RooJson;

@RooJavaBean
@RooJson
public class BigUserTransactionDto {
	private String id;
    private String type;
    private Date plattime;
    private BigDecimal amt;
    private String state;
    private String agencyno;
    private String agencyname;
    private String telephone;
	public BigUserTransactionDto(String id, String type, Date plattime,
			BigDecimal amt, String state, String agencyno,
			String agencyname, String telephone) {
		super();
		this.id = id;
		this.type = type;
		this.plattime = plattime;
		this.amt = amt;
		this.state = state;
		this.agencyno = agencyno;
		this.agencyname = agencyname;
		this.telephone = telephone;
	}
    
}
