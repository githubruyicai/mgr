function CheckboxAll_click(ckbid,ckbname) {
	for (var i = 0; i < document.getElementsByName(ckbname).length; i++) {
		document.getElementsByName(ckbname)[i].checked = document.getElementById(ckbid).checked;
	}
	document.getElementsByName(ckbname).checked = document.getElementById(ckbid).checked;
}
function setCheckBox(_checkBoxId)
{
	$(_checkBoxId).checked = 'checked';
}
